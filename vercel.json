{

    "builds": [
      {
        "src": "clg_projects/wsgi.py",
        "use": "@vercel/python",
        "config" : { "maxLambdaSize": "15mb", "runtime": "python3.7"}
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "clg_projects/wsgi.py"
      }
    ]
  }
  